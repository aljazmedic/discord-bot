{
  "name": "404_team_generator",
  "version": "0.0.1",
  "description": "Team generator bot ",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "lint": "npm run lint:js",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "civleaders": "npm run utils/civ_leader.scraper.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aljazmedic/discord-bot.git"
  },
  "keywords": [
    "discord",
    "node.js",
    "gaming"
  ],
  "author": "aljazmedic davidbes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aljazmedic/discord-bot/issues"
  },
  "homepage": "https://github.com/aljazmedic/discord-bot#readme",
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "engines": {
    "node": "12.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/plugin-syntax-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-dynamic-import-node-sync": "^2.0.1",
    "babel-plugin-styled-components": "^1.11.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^4.19.1",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "argparse": "^1.0.10",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "discord": "^0.8.2",
    "discord.js": "^12.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ytdl": "^1.3.5",
    "ytdl-core": "^3.2.1"
  },
  "nodemonConfig": {
    "ignore": [
      "lib/*.js",
      "README",
      "Bot/cache/*.json"
    ]
  }
}
